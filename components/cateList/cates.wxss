.newest-box{
    padding:0 20rpx;
}
.newest-box .newest-list{
    display: inline-block;
    width: 340rpx;
    height: 435rpx;
    margin:0 20rpx 20rpx 0;
    border-radius: 10px;
    text-align: center;
}
.newest-box .newest-list:nth-child(2n){
    margin-right: 0;
}
.newest-box .newest-list .cate-img{
    width: 335rpx;
    height: 335rpx;
    border-radius: 10px;
    margin: 20rpx 0 10rpx;
}
.newest-box .newest-list .newest-text{
    font-size: 32rpx;
}
.cate-info .touxiang-img{
   display: inline-block;
  width: 30rpx;
  height:30rpx;
  text-align: left;
  border-radius:15rpx;
}
.cate-info .user-name{
  display: inline-block;
  margin: 5rpx 0rpx auto;
  font-size: 30rpx;
  width: 50%;
  text-align: left;
}
.cate-info .zan-view{
 display: inline-block;
 text-align: left;
}
 .zan-img{
  width: 20rpx;
  height: 20rpx;
  box-align: right;
}
 .zan-score{
  font-size: 30rpx;
  color: gainsboro;
}